{
  "include": [
    "src/**/*",
    "test/**/*",
    "knex.d.ts"
  ],
  "compilerOptions": {
    "target": "es2016",
    "module": "node16",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
